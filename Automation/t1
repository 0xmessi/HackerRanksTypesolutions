    <div class="matrix-slide-wrapper has-next-prev matrix-small-screen day-matrix-wrapper">
    	<a href="javascript:void(0)" class="matrix-nav-link prev" ng-class="{'disabled':goNxtCnt==0}" ng-click="prevDayMatrix();">&nbsp;</a>
