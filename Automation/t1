<script>
	var URLS = {
		searchDayMatrixUrl : '/air-lowest-fares/dom2/getFares',
		pollUrl : {
			'dom2' : '/air-service/dom2/poll',
			'dom' : '/air-service/dom/poll',
			'b2bdom' : '/air-service/b2bdom/poll',
			'crpdom' : '/air-service/crpdom/poll',
			'int2' : '/air-service/int2/poll',
			'int' : '/air-service/int/poll',
			'b2bint' : '/air-service/b2bint/poll',
			'crpint' : '/air-service/crpint/poll'
		},

		searchUrl : {
			'dom2' : '/air-search-ui/dom2/trigger',
			'dom' : '/air-search-ui/dom/trigger',
			'b2bdom' : '/air-search-ui/b2bdom/trigger',
			'crpdom' : '/air-search-ui/crpdom/trigger',
			'int2' : '/air-search-ui/int2/trigger',
			'int' : '/air-search-ui/int2/trigger',
			'b2bint' : '/air-search-ui/b2bint/trigger',
			'crpint' : '/air-search-ui/crpint/trigger'
		},
		searchServiceUrl : {
			'int2' : '/air-service/int2/trigger',
			'dom2' : '/air-service/dom2/trigger',
			'int' : '/air-service/int/trigger',
			'dom' : '/air-service/dom/trigger',
			'b2bint' : '/air-service/b2bint/trigger',
			'crpint' : '/air-service/crpint/trigger',
			'b2bdom' : '/air-service/b2bdom/trigger',
			'crpdom' : '/air-service/crpdom/trigger'
